{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "outputMode": "new-only"
    },
    "clean": {
      "dependsOn": [],
      "cache": false
    },
    "clean:build": {
      "dependsOn": [],
      "cache": false
    }
  }
}
